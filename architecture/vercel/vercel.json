{
  "version": 2,
  "name": "ysi-catalyst-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://bool-mic-acrylic-elliott.trycloudflare.com/api/v1",
    "VITE_APP_TITLE": "YSI Catalyst Admin",
    "VITE_ENVIRONMENT": "production",
    "VITE_VERCEL_ENV": "production",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://bool-mic-acrylic-elliott.trycloudflare.com/api/v1",
      "VITE_APP_TITLE": "YSI Catalyst Admin",
      "VITE_ENVIRONMENT": "production",
      "VITE_VERCEL_ENV": "production",
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "frontend/dist/**": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}